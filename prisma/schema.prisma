generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Transaction {
  id        String    @id @default(cuid())
  descricao String
  categoria Categoria
  valor     Float
  tipo      Tipo
  data      DateTime   @default(now())
}

enum Categoria {
  Alimentacao
  Transporte
  Moradia
  Lazer
  Outros
}

enum Tipo {
  entrada
  saida
}